<div class="main-container">
    
    <%=link_to("新規投稿","/posts/new")%>
    <%= @posts.each do |post| %>
        <div class="contents">
            
                <%=link_to(post.title,"/posts/#{post.id}")%>
                <%# <img src="<%="/user_images/1.png"%>
                <%=post.content%>
                <%if Like.find_by(post_id:post.id,user_id:@current_user.id)%>
                    <%=link_to("普通","/likes/#{@post.id}/create",{methods:"post"})%>

                <%else%>
                    <%=link_to("好き","/likes/#{@post.id}/destroy")%>

                <%end%>
                <%=@likes_count%>


        
        </div>
    <% end %>

</div>